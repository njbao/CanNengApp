<view><view class="title-padding"></view><view class="history-title"><view class="history-title-text">历史数据</view></view><view class="history-block"><view class="content-block"><view data-event-opts="{{[['tap',[['jumpToDataIntegrityDetails',['$event']]]]]}}" class="content-block-line" bindtap="__e"><view class="content-block-line-left">数据完整性</view><view class="content-block-line-right"><image class="content-block-line-right-pic" src="../../static/pic/jump.png"></image></view></view><view data-event-opts="{{[['tap',[['jumpToTerminalStatus',['$event']]]]]}}" class="content-block-line" bindtap="__e"><view class="content-block-line-left">终端在线率</view><view class="content-block-line-right"><image class="content-block-line-right-pic" src="../../static/pic/jump.png"></image></view></view></view><view data-event-opts="{{[['tap',[['jumpToCondition',['$event']]]]]}}" class="sroll-block-line-one" bindtap="__e"><view class="sroll-block-line-one-text"><view>{{''+lineInfo.lineName+''}}</view><view>{{''+startDate+"至"+endDate+''}}</view></view><image class="sroll-block-line-one-pic" src="../../static/pic/jump.png"></image></view><block wx:if="{{statisticsList.length>0}}"><scroll-view class="sroll-block" style="{{'height:'+(scrollHeight)+';'}}" scroll-y="{{scrollFlag}}" data-event-opts="{{[['scrolltolower',[['queryStatistics']]],['touchend',[['touchEnd']]]]}}" bindscrolltolower="__e" bindtouchend="__e"><block wx:for="{{statisticsList}}" wx:for-item="statistics" wx:for-index="__i0__"><view data-event-opts="{{[['tap',[['jumpToTerminalHistory',['$0'],[[['statisticsList','',__i0__,'time']]]]]]]}}" class="sroll-block-content" bindtap="__e"><view class="sroll-block-content-line-one"><view class="sroll-block-content-line-one-left"><image class="sroll-block-content-line-one-left-pic" src="../../static/pic/rectangle.png"></image><view class="sroll-block-content-line-one-left-text">{{''+statistics.showTime+''}}</view></view><image class="sroll-block-content-line-one-right" src="../../static/pic/jump.png"></image></view><scroll-view class="sroll-block-content-line-text" scroll-y="true"><block wx:if="{{statistics.eventDescribe!=null}}"><view>{{''+statistics.eventDescribe+''}}</view></block><block wx:if="{{statistics.steadyDescribe!=null}}"><view>{{''+statistics.steadyDescribe+''}}</view></block></scroll-view></view></block></scroll-view></block><block wx:else><view class="no-data" style="{{'height:'+(scrollHeight)+';'}}">暂无历史数据</view></block></view></view>