<view><back vue-id="1" errorMsg="{{errorMsg}}" showFlag="{{showFlag}}" bind:__l="__l"></back><block wx:if="{{errorFlag==0}}"><view class="chooseLine-block"><view class="search-block"><view class="search-left"><image class="search-pic" src="../../static/pic/search.png"></image><input class="search-input" placeholder="请输入" confirm-type="done" type="text" placeholder-class="search-input-placeholder" data-event-opts="{{[['input',[['inputLine',['$event']]]]]}}" value="{{lineName}}" bindinput="__e"/></view><view data-event-opts="{{[['tap',[['queryByName',['$event']]]]]}}" class="search-text" bindtap="__e">搜索</view></view><view class="order-block"><view data-event-opts="{{[['tap',[['showProvince',['$event']]]]]}}" class="order-left" bindtap="__e"><view class="order-left-text">监测点选择</view><image class="order-left-pic" src="../../static/pic/pull.png"></image></view><block wx:if="{{showProvinceFlag==1}}"><block><block wx:if="{{lineFlag==1}}"><view data-event-opts="{{[['tap',[['changeLineFlag',[0]]]]]}}" class="order-center" bindtap="__e"><view class="order-center-text-choose">通讯</view><image class="order-center-pic" src="../../static/pic/desc.png"></image></view></block><block wx:else><view data-event-opts="{{[['tap',[['changeLineFlag',[1]]]]]}}" class="order-center" bindtap="__e"><view class="order-center-text">通讯</view><image class="order-center-pic" src="../../static/pic/desc.png"></image></view></block><block wx:if="{{lineFlag==2}}"><view data-event-opts="{{[['tap',[['changeLineFlag',[0]]]]]}}" class="order-right" bindtap="__e"><view class="order-right-text-choose">流量</view><image class="order-right-pic" src="../../static/pic/desc.png"></image></view></block><block wx:else><view data-event-opts="{{[['tap',[['changeLineFlag',[2]]]]]}}" class="order-right" bindtap="__e"><view class="order-right-text">流量</view><image class="order-right-pic" src="../../static/pic/desc.png"></image></view></block></block></block></view><block wx:if="{{showProvinceFlag==1}}"><view class="picker-block" style="{{'height:'+(pickerHeight)+';'}}"><view class="select-block"><block wx:if="{{provinceList.length>0}}"><picker class="select-province" mode="selector" range="{{provinceList}}" range-key="provinceName" data-event-opts="{{[['change',[['changeProvinceSelectedIndex',['$event','$0'],['provinceList.'+provinceListIndex+'.provinceIndex']]]]]}}" bindchange="__e"><view class="select-province-text">{{provinceList[provinceListIndex].provinceName}}</view></picker></block><block wx:if="{{provinceList.length>0}}"><image class="order-left-pic" src="../../static/pic/pull.png"></image></block><block wx:if="{{companyList.length>0}}"><picker class="select-company" mode="selector" range="{{companyList}}" range-key="gdName" data-event-opts="{{[['change',[['changeCompanySelectedIndex',['$event','$0'],['companyList.'+companyListIndex+'.gdIndex']]]]]}}" bindchange="__e"><view class="select-company-text">{{companyList[companyListIndex].gdName}}</view></picker></block><block wx:if="{{companyList.length>0}}"><image class="order-left-pic" src="../../static/pic/pull.png"></image></block><block wx:if="{{substationList.length>0}}"><picker class="select-substation" mode="selector" range="{{substationList}}" range-key="subName" data-event-opts="{{[['change',[['changeSubstationSelectedIndex',['$event','$0'],['substationList.'+substationListIndex+'.subIndex']]]]]}}" bindchange="__e"><view class="select-substation-text">{{substationList[substationListIndex].subName}}</view></picker></block><block wx:if="{{substationList.length>0}}"><image class="order-left-pic" src="../../static/pic/pull.png"></image></block></view><block wx:if="{{lineList.length>0}}"><scroll-view class="show-line-block" style="{{'height:'+(pickerHeight)+';'}}" scroll-y="true" show-scrollbar="false"><block wx:for="{{lineList}}" wx:for-item="line" wx:for-index="__i0__"><view data-event-opts="{{[['tap',[['selectLine',['$0','$1'],[[['lineList','',__i0__,'lineIndex']],[['lineList','',__i0__]]]]]]]}}" class="{{[lineSelectedIndex==line.lineIndex?'line-selected':'line']}}" bindtap="__e"><view class="line-text">{{''+line.lineName+''}}</view></view></block></scroll-view></block></view></block><block wx:else><scroll-view class="search-result-block" style="{{'height:'+(pickerHeight)+';'}}" scroll-y="true"><block wx:if="{{queryByNameList.length!=0}}"><view><block wx:for="{{queryByNameList}}" wx:for-item="result" wx:for-index="__i1__"><view data-event-opts="{{[['tap',[['selectLine',[1,'$0'],[[['queryByNameList','',__i1__]]]]]]]}}" class="each-block" bindtap="__e">{{''+result.lineName+''}}</view></block></view></block><block wx:else><view class="each-block">{{''+searchResult+''}}</view></block></scroll-view></block></view></block><block wx:else><view class="error-block"><view class="error-text">网络出错请重试</view><button data-event-opts="{{[['tap',[['retry',['$event']]]]]}}" class="retry_button" bindtap="__e">重试</button></view></block></view>